<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="375dp"
        android:layout_height="69dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Descrição da Tarefa"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="141dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="19dp"
        android:layout_height="19dp"
        app:srcCompat="@drawable/_324826"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="581dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nome da Tarefa"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="78dp" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="350dp"
        android:layout_height="303dp"
        tools:layout_editor_absoluteX="23dp"
        tools:layout_editor_absoluteY="228dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Organizar arquivos no GitHub"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="581dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="19dp"
        android:layout_height="19dp"
        app:srcCompat="@drawable/_334328"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="581dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="411dp"
        android:layout_height="66dp"
        android:background="#6B0048FF"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <TextView
            android:layout_width="244dp"
            android:layout_height="32dp"
            android:text="Gerenciamento de Tarefas"
            android:textColor="#000000"
            android:textSize="20sp"
            app:layout_constraintStaret_toStartOf="parent"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="18dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="37dp"
            android:layout_height="30dp"
            app:srcCompat="@drawable/_582717"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="20dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="#91AFFC"
        app:srcCompat="@drawable/_0310"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="641dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="#93B1FE"
        app:srcCompat="@drawable/_334328"
        tools:layout_editor_absoluteX="310dp"
        tools:layout_editor_absoluteY="641dp" />

</androidx.constraintlayout.widget.ConstraintLayout>